{
  "name": "mgonzalezbaile/merge-pull-request-pm",
  "description": "Manage the process of merging a PR",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "soa-php/message-store-amqp": "^1.0",
    "soa-php/message-store-mongodb": "^1.0",
    "soa-php/process-manager-middleware": "^1.0",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-config-aggregator": "^1.1",
    "zendframework/zend-component-installer": "^2.1.1",
    "zendframework/zend-diactoros": "^1.7.1 || ^2.0",
    "zendframework/zend-expressive": "^3.0.1",
    "zendframework/zend-expressive-helpers": "^5.0",
    "zendframework/zend-stdlib": "^3.1",
    "zendframework/zend-expressive-fastroute": "^3.0",
    "zendframework/zend-problem-details": "^1.0",
    "lukasoppermann/http-status": "^2.0",
    "martinezdelariva/hydrator": "^1.0",
    "martinezdelariva/functional": "dev-master",
    "filp/whoops": "^2.3",
    "moneyphp/money": "^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "MergePullRequestPm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MergePullRequestPmTest\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
