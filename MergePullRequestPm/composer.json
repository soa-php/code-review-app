{
  "name": "mgonzalezbaile/merge-pull-request-pm",
  "description": "Manage the process of merging a PR",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "mgonzalezbaile/common": "dev-user-identity-bc",
    "soa-php/process-manager-middleware": "^1.0",
    "martinezdelariva/functional": "dev-master",
    "ext-json": "*",
    "moneyphp/money": "^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12",
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "MergePullRequestPm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MergePullRequestPmTest\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../Common",
      "options": {
        "symlink": false
      }
    }
  ],
  "config": {
    "bin-dir": "bin/"
  }
}
